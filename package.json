{
  "name": "study-with-miku",
  "version": "0.0.1",
  "description": "Study with Miku - 和Miku一起学习吧~",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "vite:build": "vite build",
    "generate-icons": "node scripts/generate-icons.js",
    "build": "npm run generate-icons && npm run vite:build && npm run copy-assets",
    "preview": "vite preview",
    "copy-assets": "copyfiles -u 1 public/**/* dist/",
    "dev:worker": "npm run build && wrangler dev --local",
    "deploy:worker": "wrangler deploy"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@vueuse/core": "^10.1.2",
    "aplayer": "^1.10.1",
    "hono": "^4.11.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.83",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/ph": "^1.2.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "copyfiles": "^2.4.1",
    "sass-embedded": "^1.97.1",
    "sharp": "^0.33.0",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0",
    "wrangler": "^4.53.0"
  }
}
